# Anthropic API Key (required for anthropic provider)
ANTHROPIC_API_KEY=sk-ant-xxxxx

# Server port (default: 4000)
PORT=4000

# Projects directory (default: ./projects)
PROJECTS_DIR=./projects

# LLM Provider (anthropic, openrouter, openai, gemini, deepseek, groq, mistral)
API_PROVIDER=anthropic

# Model to use (default: claude-sonnet-4-6)
API_MODEL_ID=claude-sonnet-4-6

# Optional: Other provider API keys
# OPENROUTER_API_KEY=sk-or-xxxxx
# OPENAI_API_KEY=sk-xxxxx
# GOOGLE_API_KEY=xxxxx
# DEEPSEEK_API_KEY=xxxxx
# GROQ_API_KEY=xxxxx
# MISTRAL_API_KEY=xxxxx
